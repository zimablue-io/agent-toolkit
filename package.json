{
  "name": "@zimablue-io/agent-toolkit",
  "version": "0.1.0",
  "description": "A framework for building AI agents with specialized tools and capabilities",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "openai",
    "gpt",
    "ai-agents",
    "agent-toolkit",
    "ai-framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zimablue-io/agent-toolkit.git"
  },
  "author": "zimablue-io",
  "bugs": {
    "url": "https://github.com/zimablue-io/agent-toolkit/issues"
  },
  "homepage": "https://github.com/zimablue-io/agent-toolkit#readme",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "dependencies": {
    "express": "5.1.0",
    "openai": "4.95.0",
    "winston": "3.17.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "@types/node": "22.14.1",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "eslint": "9.24.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "typescript": "5.8.3",
    "vitest": "3.1.1"
  }
}
